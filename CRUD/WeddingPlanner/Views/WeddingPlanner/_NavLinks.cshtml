@model ViewModel

<div class="d-flex justify-content-between">
    
    <ul class="d-flex list-unstyled">
        <li>
            <a asp-controller="WeddingPlanner" asp-action="Dashboard" asp-route-id="@Context.Session.GetInt32("UserId")" class="@(Model.CurrentView == "Dashboard" ? "disabled" : "")">Home</a> | 
        </li>
        <li>
            <a href="/weddings/new" class="@(Model.CurrentView == "NewWedding" ? "disabled" : "")"> Plan a Wedding</a> | 
        </li>
        <li>
            <form asp-controller="User" asp-action="Logout" method="post"><button type="submit" class="btn text-primary btn-link p-0"> Logout</button></form>
        </li>
    </ul>
    
</div>