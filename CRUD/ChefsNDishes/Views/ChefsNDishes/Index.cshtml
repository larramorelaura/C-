@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<dynamic>
<div class="text-center">
    @await Html.PartialAsync("_NavLinks", new ChefsNDishes.Models.ViewModel{CurrentView="Chefs"} )
    <h1>Check out our roster of chefs!</h1>
    <table class="table table-striped">
        <thead>
            <th>Name</th>
            <th>Age</th>
            <th>Number of Dishes</th>
        </thead>
        <tbody>
            @foreach (dynamic u in Model)
            {
                <tr>
                    <td>@u.FirstName</td>
                    <td>@u.Age</td>
                    <td>@u.DishCount</td>
                </tr>
            }
        </tbody>
    </table>
</div>
